# متغيرات بيئة Frontend لـ React
# قم بنسخ هذا الملف إلى .env في مجلد frontend/my-project-main

# URL الخاص بـ Laravel Backend API
REACT_APP_API_URL=http://127.0.0.1:8000

# Google OAuth Client ID (نفس القيمة المستخدمة في Backend)
REACT_APP_GOOGLE_CLIENT_ID=your-google-client-id-here.apps.googleusercontent.com

# اختياري: URL للإعادة التوجيه بعد تسجيل الدخول
REACT_APP_REDIRECT_URL=http://localhost:3000/auth/google/callback

# بيئة التطوير
NODE_ENV=development

# Port للتطوير (اختياري)
PORT=3000

# API Base URL (for compatibility with newer versions)
REACT_APP_API_BASE_URL=http://127.0.0.1:8000/api